{
  "version": "1.0.0",
  "name": "benchmarks",
  "scripts": {
    "bench:javascript:console": "cd benchmarks/javascript/console && bun run bench",
    "bench:javascript:3rd-packages": "cd benchmarks/javascript/3rd-packages && bun run bench",
    "bench:javascript:json": "cd benchmarks/javascript/json && bun run bench",
    "bench:javascript:buffer": "cd benchmarks/javascript/buffer && bun run bench",
    "bench:javascript:http": "cd benchmarks/javascript/http && bun run bench",
    "bench:javascript:sqlite": "cd benchmarks/javascript/sqlite && bun run bench",
    "bench": "bun run bench:javascript:console && bun run bench:javascript:3rd-packages && bun run bench:javascript:json && bun run bench:javascript:buffer && bun run bench:javascript:http && bun run bench:javascript:sqlite",
    "generate:readme": "bun scripts/readme.ts"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "bun-types": "^0.1.4"
  },
  "dependencies": {
    "bun-utilities": "^0.1.8",
    "markdown-table": "^3.0.2",
    "mitata": "^0.1.6"
  }
}